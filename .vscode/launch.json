{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "obelist",
      "type": "python",
      "request": "launch",
      "program": "${workspaceRoot}/.venv/bin/obelist",
      "console": "integratedTerminal"
    },
    {
      "name": "obelist path",
      "type": "python",
      "request": "launch",
      "program": "${workspaceRoot}/.venv/bin/obelist",
      "args": ["path"],
      "cwd": "${workspaceRoot}/src/obelist",
      "console": "integratedTerminal"
    },
    {
      "name": "obelist debug",
      "type": "python",
      "request": "launch",
      "program": "${workspaceRoot}/.venv/bin/obelist",
      "args": ["debug", "foo"],
      "console": "integratedTerminal"
    },
    {
      "name": "obelist parse gcc",
      "type": "python",
      "request": "launch",
      "program": "${workspaceRoot}/.venv/bin/obelist",
      "args": [
        "parse",
        "-q",
        "-p",
        "shellcheck",
        "-f",
        "gcc",
        "test/output/shellcheck/gcc.txt"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "obelist parse checkstyle",
      "type": "python",
      "request": "launch",
      "program": "${workspaceRoot}/.venv/bin/obelist",
      "args": [
        "parse",
        "-q",
        "-p",
        "shellcheck",
        "-f",
        "checkstyle",
        "test/output/shellcheck/checkstyle.xml"
      ],
      "console": "integratedTerminal"
    },
    {
      "name": "obelist parse json",
      "type": "python",
      "request": "launch",
      "program": "${workspaceRoot}/.venv/bin/obelist",
      "args": [
        "parse",
        "-q",
        "-p",
        "shellcheck",
        "-f",
        "json",
        "test/output/shellcheck/json.json"
      ],
      "console": "integratedTerminal"
    }
  ]
}
